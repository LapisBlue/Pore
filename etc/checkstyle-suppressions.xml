<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suppressions PUBLIC "-//Puppy Crawl//DTD Suppressions 1.1//EN" "http://www.puppycrawl.com/dtds/suppressions_1_1.dtd">

<suppressions>
    <suppress checks=".*" files="[\\/]lib[\\/]"/>
    <suppress checks="JavadocMethod" files="^src[\\/](main|test)[\\/]java"/>
    <suppress checks="Indentation" files="package-info\.java"/>
    <suppress checks="AbbreviationAsWordInName" files="TNT"/>
    <suppress checks="Indentation" files=".*"/> <!-- TODO: this check doesn't like lambdas -->
</suppressions>
